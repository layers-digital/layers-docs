              "required": false
            },
            "community": {
              "type": "string",
              "description": "Identificador da communidade do membro",
              "example": "test",
              "readOnly": true
            },
            "access": {
              "type": "array",
              "required": false,
              "items": {
                "type": "object",
                "properties": {
                  "permissions": {
                    "type": "array",
