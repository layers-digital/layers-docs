              "properties": {
                "name": {
                  "type": "string",
                  "description": "Nome do autor"
                },
                "email": {
                  "type": "string",
                  "description": "Email do autor"
                },
                "alias": {
                  "type": "string",
                  "description": "Identificador interno do autor"
                }
              }
            },
            "attachments": {
              "type": "array",
