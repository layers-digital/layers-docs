                "group": {
                  "type": "string",
                  "format": "ObjectId",
                  "description": "Identificador do grupo à qual pertence a matrícula"
                },
                "createdAt": {
                  "type": "string",
                  "format": "date-time",
                  "description": "Data de criação da matrícula"
