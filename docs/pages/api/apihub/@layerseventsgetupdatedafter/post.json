  "responseSchema": {
    "title": "LayersEventsGetUpdatedAfterResponse",
    "type": "object",
    "required": [
      "result"
    ],
    "properties": {
      "result": {
        "type": "array",
        "description": "Eventos da agenda",
        "items": {
          "type": "object",
          "required": [
            "title",
            "createdAt",
            "updatedAt",
            "targets",
            "start",
            "end"
          ],
          "properties": {
            "id": {
              "type": "string",
              "description": "Identificador interno do evento",
              "example": "0001"
            },
            "title": {
              "type": "string",
              "description": "Nome do evento",
              "example": "Prova 1"
            },
            "description": {
              "type": "string",
              "description": "Descrição do evento",
              "example": "A prova deve ser realizada presencialmente"
            },
            "createdAt": {
              "type": "string",
              "format": "date-time",
              "description": "Data e hora de criação do evento"
            },
            "updatedAt": {
              "type": "string",
              "format": "date-time",
              "description": "Data e hora da ultima atualização do evento"
            },
            "category": {
              "type": "string",
              "description": "Categorias associadas aos eventos. São usadas para definir a cor e as permissões alvo de cada evento. Caso um evento tenha uma categoria que não foi pré definida pela comunidade o evento não será sincronizado",
              "example": "Geral"
            },
            "targets": {
              "type": "object",
              "description": "Alvos para quem serão publicados o evento",
              "properties": {
                "users": {
                  "type": "array",
                  "description": "Lista de identificadores únicos de usuários",
