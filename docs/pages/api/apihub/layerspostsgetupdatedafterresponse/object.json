  "schema": {
    "type": "schema",
    "name": "LayersPostsGetUpdatedAfterResponse",
    "spec": {
      "type": "object",
      "required": [
        "result"
      ],
      "properties": {
        "result": {
          "type": "array",
          "description": "Publicações do app comunicados",
          "items": {
            "type": "object",
            "required": [
              "title",
              "description",
              "createdAt",
              "updatedAt",
              "targets"
            ],
            "properties": {
              "id": {
                "type": "string",
                "description": "Identificador interno da publicação",
                "example": "0001"
              },
              "title": {
                "type": "string",
                "description": "Titulo da publicação",
                "example": "Comunicado ano letivo 2021"
              },
              "description": {
                "type": "string",
                "description": "Descrição/conteúdo da publicação",
                "example": "O ano letivo já vai começar! A partir da segunda que vem, no dia 01 de fevereiro estaremos aguardando vocês."
              },
              "createdAt": {
                "type": "string",
                "format": "date-time",
                "description": "Data e hora de criação da publicação"
              },
              "updatedAt": {
                "type": "string",
                "format": "date-time",
                "description": "Data e hora da ultima atualização da publicação"
              },
              "category": {
                "type": "string",
                "description": "Categorias associadas as publicações. São usadas para definir a cor e as permissões alvo de cada publicação. Caso uma publicação tenha uma categoria que não foi pré definida pela comunidade a publicação não será sincronizada",
                "example": "Geral"
              },
              "targets": {
                "type": "object",
                "description": "Alvos para quem serão enviadas as publicações",
                "properties": {
                  "users": {
                    "type": "array",
                    "description": "Lista de identificadores únicos de usuários",
                    "items": {
                      "type": "string",
                      "nullable": true
                    }
                  },
                  "members": {
                    "type": "array",
                    "description": "Lista de identificadores únicos de membros",
                    "items": {
                      "type": "string",
                      "nullable": true
                    }
                  },
                  "groups": {
                    "type": "array",
                    "description": "Lista de identificadores únicos de grupos",
                    "items": {
                      "type": "string",
                      "nullable": true
                    }
                  }
                }
              },
              "author": {
                "type": "object",
                "description": "Autor da publicação",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Nome do autor"
                  },
                  "email": {
                    "type": "string",
                    "description": "Email do autor"
                  },
                  "alias": {
                    "type": "string",
                    "description": "Identificador interno do autor"
                  }
                }
              },
              "attachments": {
                "type": "array",
                "description": "Lista de anexos da publicação",
                "items": {
                  "type": "object",
                  "required": [
                    "title",
                    "type",
                    "url"
                  ],
                  "properties": {
                    "title": {
                      "type": "string",
                      "description": "Título do anexo",
                      "example": "CalculoDeNotas.pdf"
                    },
                    "type": {
                      "type": "string",
                      "enum": [
                        "file",
                        "link",
                        "image"
                      ],
                      "description": "Tipo do anexo",
                      "example": "file`"
                    },
                    "url": {
                      "type": "string",
                      "description": "URL para fazer download do anexo. Obrigatóriamente uma url HTTPS",
                      "example": "https://calculo.pdf"
                    }
                  }
                }
              },
              "approved": {
                "type": "boolean",
                "description": "Indica se o comunicado deve ser publicado diretamente ou deve passar por aprovação de um administrador da comunidade",
                "example": true
              }
            }
          }
        }
      },
